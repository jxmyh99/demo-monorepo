#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules/nano-staged/lib/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules/nano-staged/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules/nano-staged/lib/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules/nano-staged/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/nano-staged@0.8.0/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/nano-staged@0.8.0/node_modules/nano-staged/lib/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/nano-staged@0.8.0/node_modules/nano-staged/lib/bin.js" "$@"
fi
