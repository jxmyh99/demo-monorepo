#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules/@commitlint/cli/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules/@commitlint/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules/@commitlint/cli/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules/@commitlint/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.4/node_modules:/mnt/d/gsworking/gitlab/demo-project/monorepo/monorepo-1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
